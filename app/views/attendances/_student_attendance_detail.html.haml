%tbody
  %tr.height-30{data: {attendance_id: attendance.id}}
    %td
      %strong.lmargin5="#{attendance.user_id} - #{attendance.id}"
    %td
      .attendance-date=attendance.attendance_date
    - @attendance_types.each do |at|
      %td.centered{data: {type_id: at.id}}=by_type[at.id] if by_type[at.id] > 0
    - if deact_column == 1
      %td.centered{data: {type_id: 9999999}}=by_type[9999999]
%tbody.tbody-body
